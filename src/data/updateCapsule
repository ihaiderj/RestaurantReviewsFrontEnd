                                                    record.interiorLightBeltImage = uploadedFiles[0].fileUrl; // Save the uploaded image URL
                                                    $w('#updateInteriorLInteriorLBC4').src = uploadedFiles[0].fileUrl;
                                                }
                                            });
                                        uploadPromises.push(interiorLightBeltImagePromise);
                                    }

                                    if (interiorCeilingStripLights1.value.length > 0) {
                                        // Upload the AC image
                                        const interiorCeilingStripLights1Promise = interiorCeilingStripLights1.uploadFiles()
                                            .then(async (uploadedFiles) => {
                                                if (uploadedFiles.length > 0) {
                                                    let id = uploadedFiles[0].fileName // here fileName is actually fileID as required and explained in the docs
                                                    let newFileName = generateNewImageName("interior-lighting-ceiling-strip-lights")
                                                    let fileUpdates = {
                                                        displayName: newFileName,
                                                        label: ['interior-lighting', 'spaceCapsules', 'ceiling-strip-lights'],
                                                    }
                                                    const updatedLocation = await myUpdateFileDescriptorFunction(id, fileUpdates);

                                                    record.interiorCeilingStripLights1 = uploadedFiles[0].fileUrl; // Save the uploaded image URL
                                                    $w('#updateInteriorLInteriorCSLC4').src = uploadedFiles[0].fileUrl;
                                                }
                                            });
                                        uploadPromises.push(interiorCeilingStripLights1Promise);
                                    }

                                    if (interiorFloorStripLightsImage.value.length > 0) {
                                        // Upload the AC image
                                        const interiorFloorStripLightsImagePromise = interiorFloorStripLightsImage.uploadFiles()
                                            .then(async (uploadedFiles) => {
                                                if (uploadedFiles.length > 0) {
                                                    let id = uploadedFiles[0].fileName // here fileName is actually fileID as required and explained in the docs
                                                    let newFileName = generateNewImageName("interior-lighting-floor-strip-lights")
                                                    let fileUpdates = {
                                                        displayName: newFileName,
                                                        label: ['interior-lighting', 'spaceCapsules', 'floor-strip-lights'],
                                                    }
                                                    const updatedLocation = await myUpdateFileDescriptorFunction(id, fileUpdates);

                                                    record.interiorFloorStripLightsImage = uploadedFiles[0].fileUrl; // Save the uploaded image URL
                                                    $w('#updateInteriorLInteriorFSLC4').src = uploadedFiles[0].fileUrl;
                                                }
                                            });
                                        uploadPromises.push(interiorFloorStripLightsImagePromise);
                                    }

                                    if (interiorFeatureWallLightsImage.value.length > 0) {
                                        // Upload the AC image
                                        const interiorFeatureWallLightsImagePromise = interiorFeatureWallLightsImage.uploadFiles()
                                            .then(async (uploadedFiles) => {
                                                if (uploadedFiles.length > 0) {
                                                    let id = uploadedFiles[0].fileName // here fileName is actually fileID as required and explained in the docs
                                                    let newFileName = generateNewImageName("interior-lighting-feature-wall-lights")
                                                    let fileUpdates = {
                                                        displayName: newFileName,
                                                        label: ['interior-lighting', 'spaceCapsules', 'feature-wall-lights'],
                                                    }
                                                    const updatedLocation = await myUpdateFileDescriptorFunction(id, fileUpdates);

                                                    record.interiorFeatureWallLightsImage = uploadedFiles[0].fileUrl; // Save the uploaded image URL
                                                    $w('#updateInteriorLInteriorFWLC4').src = uploadedFiles[0].fileUrl;
                                                }
                                            });
                                        uploadPromises.push(interiorFeatureWallLightsImagePromise);
                                    }

                                    if (otherInteriorLightingImage.value.length > 0) {
                                        // Upload the AC image
                                        const otherInteriorLightingImagePromise = otherInteriorLightingImage.uploadFiles()
                                            .then(async (uploadedFiles) => {
                                                if (uploadedFiles.length > 0) {
                                                    let id = uploadedFiles[0].fileName // here fileName is actually fileID as required and explained in the docs
                                                    let newFileName = generateNewImageName("interior-lighting-other-lights")
                                                    let fileUpdates = {
                                                        displayName: newFileName,
                                                        label: ['interior-lighting', 'spaceCapsules', 'other-lights'],
                                                    }
                                                    const updatedLocation = await myUpdateFileDescriptorFunction(id, fileUpdates);

                                                    record.otherInteriorLightingImage = uploadedFiles[0].fileUrl; // Save the uploaded image URL
                                                    $w('#updateInteriorLOtherC4').src = uploadedFiles[0].fileUrl;
                                                }
                                            });
                                        uploadPromises.push(otherInteriorLightingImagePromise);
                                    }

                                    // Wait for all image uploads to complete
                                    return Promise.all(uploadPromises).then(() => record);

                                })
                                .then((updatedRecord) => {
                                    // Save the updated record back to the collection
                                    return wixData.update("SpaceCapsuleFeatures", updatedRecord);
                                })
                                .then((result) => {
                                            $w('#submitInteriorLighting').enable();
                                            showBoxSuccessMessage("Capsule Interior Lighting Information Added Successfully")

                                })
                                .catch((error) => {
                                    console.error("Error updating interior lighting Section", error);
                                    showBoxErrorMessage(error.errorDesciption)
                                    $w('#submitInteriorLighting').enable();
                                });

                
                } 

            }      

            async function submitExteriorLighting(params) {
                $w('#loader').show();
                $w('#submitExteriorLighting').disable();
                const isformValid = await validateForm();
                if (!isformValid) {
                    $w('#submitExteriorLighting').enable();
                    return;
                } 
                else {
                const ledWindowStripLightsImage = $w('#updateExteriorLLEDWSLC4')
                const featureWallLightsImage = $w('#updateExteriorLFeatureWLC4')
                const mainDoorLightsImage = $w('#updateExteriorLMainDLC4')
                const balconyLightsImage = $w('#updateExteriorLBalconyLC4')
                const externalFloorLightsImage = $w('#updateExteriorLExternalFLC4')
                const otherExteriorLightingImage = $w('#updateExteriorLOtherC4')

                const ledWindowStripLights = await fieldValueWithC1C2($w('#updateExteriorLLEDWSLNo'), 'CapsuleExteriorLightingQS', 'LED Window Strip Lights', 'exteriorLightingFeatures');
                const featureWallLights = await fieldValueWithC1C2($w('#updateExteriorLFeatureWLNo'), 'CapsuleExteriorLightingQS', 'Feature Wall Lights', 'exteriorLightingFeatures');
                const mainDoorLights = await fieldValueWithC1C2($w('#updateExteriorLMainDLNo'), 'CapsuleExteriorLightingQS', 'Main Door Lights', 'exteriorLightingFeatures');
                const balconyLights = await fieldValueWithC1C2($w('#updateExteriorLBalconyLNo'), 'CapsuleExteriorLightingQS', 'Balcony Lights', 'exteriorLightingFeatures');
                const externalFloorLights = await fieldValueWithC1C2($w('#updateExteriorLExternalFLNo'), 'CapsuleExteriorLightingQS', 'External Floor Lights', 'exteriorLightingFeatures');
                const otherExteriorLighting = await fieldValueWithC1C2($w('#updateExteriorLOtherNo'), 'CapsuleExteriorLightingQS', 'Other', 'exteriorLightingFeatures');
                const totalScore = (ledWindowStripLights.score + featureWallLights.score + mainDoorLights.score + balconyLights.score
                + externalFloorLights.score + otherExteriorLighting.score)

                const modelName = $w('#updateMainCIModelC1').value
                const brandName = $w('#updateMainCIBrandC1').value
                savedBrand = brandName;
                savedModel = modelName


                wixData.get('SpaceCapsuleFeatures', productID)
                                .then((record) => {
                                    // Update only the fields you want to change
                                    let uploadPromises = []
                                    record.ledWindowStripLights = ledWindowStripLights.choice
                                    record.ledWindowStripLightsQualityScore   = ledWindowStripLights.score
                                    record.featureWallLight = featureWallLights.choice
                                    record.featureWallLightsQualityScore  = featureWallLights.score
                                    record.mainDoorLights = mainDoorLights.choice
                                    record.mainDoorLightsQualityScore  = mainDoorLights.score
                                    record.balconyLights = balconyLights.choice
                                    record.balconyLightsQualityScore  = balconyLights.score
                                    record.externalFloorLights = externalFloorLights.choice
                                    record.externalFloorLightsQualityScore  = externalFloorLights.score
                                    record.otherExteriorLighting = otherExteriorLighting.choice
                                    record.otherExteriorLightingQualityScore  = otherExteriorLighting.score
                                    
                                    const prevTotalQS = record.totalExteriorLightingQualityScore
                                  
                                    record.totalExteriorLightingQualityScore  = totalScore
                                    
                                    record.totalQualityScore = record.totalQualityScore + totalScore - prevTotalQS

                                    
                                    if (ledWindowStripLightsImage.value.length > 0) {
                                        // Upload the AC image
                                        const ledWindowStripLightsImagePromise = ledWindowStripLightsImage.uploadFiles()
                                            .then(async (uploadedFiles) => {
                                                if (uploadedFiles.length > 0) {
                                                    let id = uploadedFiles[0].fileName // here fileName is actually fileID as required and explained in the docs
                                                    let newFileName = generateNewImageName("exterior-lighting-led-window-strip-lights")
                                                    let fileUpdates = {
                                                        displayName: newFileName,
                                                        label: ['exterior-lighting', 'spaceCapsules', 'led-window-strip-lights'],
                                                    }
                                                    const updatedLocation = await myUpdateFileDescriptorFunction(id, fileUpdates);

                                                    record.ledWindowStripLightsImage = uploadedFiles[0].fileUrl; // Save the uploaded image URL
                                                    $w('#updateExteriorLLEDWSLC4').src = uploadedFiles[0].fileUrl;
                                                }
                                            });
                                        uploadPromises.push(ledWindowStripLightsImagePromise);
                                    }

                                    if (featureWallLightsImage.value.length > 0) {
                                        // Upload the AC image
                                        const featureWallLightsImagePromise = featureWallLightsImage.uploadFiles()
                                            .then(async (uploadedFiles) => {
                                                if (uploadedFiles.length > 0) {
                                                    let id = uploadedFiles[0].fileName // here fileName is actually fileID as required and explained in the docs
                                                    let newFileName = generateNewImageName("exterior-lighting-feature-wall-lights")
                                                    let fileUpdates = {
                                                        displayName: newFileName,
                                                        label: ['exterior-lighting', 'spaceCapsules', 'feature-wall-lights'],
                                                    }
                                                    const updatedLocation = await myUpdateFileDescriptorFunction(id, fileUpdates);

                                                    record.featureWallLightsImage = uploadedFiles[0].fileUrl; // Save the uploaded image URL
                                                    $w('#updateExteriorLFeatureWLC4').src = uploadedFiles[0].fileUrl;
                                                }
                                            });
                                        uploadPromises.push(featureWallLightsImagePromise);
                                    }

                                    if (mainDoorLightsImage.value.length > 0) {
                                        // Upload the AC image
                                        const mainDoorLightsImagePromise = mainDoorLightsImage.uploadFiles()
                                            .then(async (uploadedFiles) => {
                                                if (uploadedFiles.length > 0) {
                                                    let id = uploadedFiles[0].fileName // here fileName is actually fileID as required and explained in the docs
                                                    let newFileName = generateNewImageName("exterior-lighting-led-window-strip-lights")
                                                    let fileUpdates = {
                                                        displayName: newFileName,
                                                        label: ['exterior-lighting', 'spaceCapsules', 'led-window-strip-lights'],
                                                    }
                                                    const updatedLocation = await myUpdateFileDescriptorFunction(id, fileUpdates);

                                                    record.mainDoorLightsImage = uploadedFiles[0].fileUrl; // Save the uploaded image URL
                                                    $w('#updateExteriorLMainDLC4').src = uploadedFiles[0].fileUrl;
                                                }
                                            });
                                        uploadPromises.push(mainDoorLightsImagePromise);
                                    }

                                    if (balconyLightsImage.value.length > 0) {
                                        // Upload the AC image
                                        const balconyLightsImagePromise = balconyLightsImage.uploadFiles()
                                            .then(async (uploadedFiles) => {
                                                if (uploadedFiles.length > 0) {
                                                    let id = uploadedFiles[0].fileName // here fileName is actually fileID as required and explained in the docs
                                                    let newFileName = generateNewImageName("exterior-lighting-balcony-lights")
                                                    let fileUpdates = {
                                                        displayName: newFileName,
                                                        label: ['exterior-lighting', 'spaceCapsules', 'balcony-lights'],
