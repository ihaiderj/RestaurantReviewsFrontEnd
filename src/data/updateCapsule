                    mainString = element.id;
                    diffString = 'Yes'
                    let stringTosearch = extractBeforeSubstring(onchnageelementid, diffString);
                    if (element.checked) {
                        for (let i = 0; i <= warenteesNo.length - 1; i++) {
                            let found = findSubstring(warenteesNo[i].id, stringTosearch)
                            if (found) {
                                warenteesNo[i].checked = false
                            }
                        }
                        for (let i = 0; i <= warenteesC1.length - 1; i++) {
                            let elementTosearch = stringTosearch + 'C1'
                            if (warenteesC1[i].id === elementTosearch) {
                                warenteesC1[i].show();

                            }
                        }
                        for (let c = 0; c <= warenteesC2.length - 1; c++) {
                            stringTosearch = extractBeforeSubstring(event.target.id, 'Yes')
                            let elementToSearch = stringTosearch + 'C2'
                            let elementToOpen = stringTosearch + 'C5'
                            if (warenteesC2[c].id === elementToSearch) {
                                warenteesC2[c].onChange(() => {
                                    for (let d = 0; d < warenteesC3.length; d++) {
                                        if (warenteesC3[d].id === elementToOpen) {
                                            warenteesC3[d].show();
                                            break;
                                        }
                                    }
                                })

                            }

                        }

                    } else {
                        element.checked = true;

                    }
                })
            }
            for (let t = 0; t <= naturalGasYesElements.length - 1; t++) {
                let element = naturalGasYesElements[t]
