                                                    record.tapwareColourImage1 = uploadedFiles[0].fileUrl; // Save the uploaded image URL
                                                    $w('#updateBathroomBathroomTCC4').src = uploadedFiles[0].fileUrl;
                                                }
                                            });
                                        uploadPromises.push(tapwareColourImagePromise);
                                    }
                                    if (bathroomBenchMaterialImage.value.length > 0) {
                                        // Upload the AC image
                                        const bathroomBenchMaterialImagePromise = bathroomBenchMaterialImage.uploadFiles()
                                            .then(async (uploadedFiles) => {
                                                if (uploadedFiles.length > 0) {
                                                    let id = uploadedFiles[0].fileName // here fileName is actually fileID as required and explained in the docs
                                                    let newFileName = generateNewImageName("bathroom-bench-material")
                                                    let fileUpdates = {
                                                        displayName: newFileName,
                                                        label: ['bathroom', 'spaceCapsules', 'bench-material'],
                                                    }
                                                    const updatedLocation = await myUpdateFileDescriptorFunction(id, fileUpdates);
                                                    record.bathroomBenchMaterialImage = uploadedFiles[0].fileUrl; // Save the uploaded image URL
                                                    $w('#updateBathroomBathroomBMC4').src = uploadedFiles[0].fileUrl;
                                                }
                                            });
                                        uploadPromises.push(bathroomBenchMaterialImagePromise);
                                    }
                                    if (towelRailImage.value.length > 0) {
                                        // Upload the AC image
                                        const towelRailImagePromise = towelRailImage.uploadFiles()
                                            .then(async (uploadedFiles) => {
                                                if (uploadedFiles.length > 0) {
                                                    let id = uploadedFiles[0].fileName // here fileName is actually fileID as required and explained in the docs
                                                    let newFileName = generateNewImageName("bathroom-towel-rail")
                                                    let fileUpdates = {
                                                        displayName: newFileName,
                                                        label: ['bathroom', 'spaceCapsules', 'towel-rail'],
                                                    }
                                                    const updatedLocation = await myUpdateFileDescriptorFunction(id, fileUpdates);

                                                    record.towelRailImage = uploadedFiles[0].fileUrl; // Save the uploaded image URL
                                                    $w('#updateBathroomTowelRC4').src = uploadedFiles[0].fileUrl;
                                                }
                                            });
                                        uploadPromises.push(towelRailImagePromise);
                                    }
                                    if (exhaustFanImage.value.length > 0) {
                                        // Upload the AC image
                                        const exhaustFanImagePromise = exhaustFanImage.uploadFiles()
                                            .then(async (uploadedFiles) => {
                                                if (uploadedFiles.length > 0) {
                                                    let id = uploadedFiles[0].fileName // here fileName is actually fileID as required and explained in the docs
                                                    let newFileName = generateNewImageName("bathroom-exhaust-fan")
                                                    let fileUpdates = {
                                                        displayName: newFileName,
                                                        label: ['bathroom', 'spaceCapsules', 'exhaust-fan'],
                                                    }
                                                    const updatedLocation = await myUpdateFileDescriptorFunction(id, fileUpdates);

                                                    record.exhaustFanImage = uploadedFiles[0].fileUrl; // Save the uploaded image URL
                                                    $w('#updateBathroomExhaustFC4').src = uploadedFiles[0].fileUrl;
