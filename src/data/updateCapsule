                                                    $w('#updateAppliancesCooktopIC4').src = uploadedFiles[0].fileUrl;
                                                }
                                            });
                                        uploadPromises.push(cooktopImagePromise);
                                    }
                                    if (dishwasherImage.value.length > 0) {
                                        // Upload the AC image
                                        const dishwasherImagePromise = dishwasherImage.uploadFiles()
                                            .then(async (uploadedFiles) => {
                                                if (uploadedFiles.length > 0) {
                                                    let id = uploadedFiles[0].fileName // here fileName is actually fileID as required and explained in the docs
                                                    let newFileName = generateNewImageName("appliances-dishwahser")
                                                    let fileUpdates = {
                                                        displayName: newFileName,
                                                        label: ['appliances', 'spaceCapsules', 'dishwahser'],
                                                    }
