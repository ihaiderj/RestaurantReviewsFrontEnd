                                                    $w('#updateAppliancesMicrowaveIC4').src = uploadedFiles[0].fileUrl;
                                                }
                                            });
                                        uploadPromises.push(microwaveImagePromise);
                                    }
                                    if (fridgeImage.value.length > 0) {
                                        // Upload the AC image
                                        const fridgeImagePromise = fridgeImage.uploadFiles()
                                            .then(async (uploadedFiles) => {
                                                if (uploadedFiles.length > 0) {
                                                    let id = uploadedFiles[0].fileName // here fileName is actually fileID as required and explained in the docs
                                                    let newFileName = generateNewImageName("appliances-firdge")
