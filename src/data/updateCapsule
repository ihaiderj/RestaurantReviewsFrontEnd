                                                    $w('#updateInteriorLInteriorILSC4').src = uploadedFiles[0].fileUrl;
                                                }
                                            });
                                        uploadPromises.push(interiorInternalLightSystemImagePromise);
                                    }

                                    if (interiorLightBeltImage.value.length > 0) {
                                        // Upload the AC image
                                        const interiorLightBeltImagePromise = interiorLightBeltImage.uploadFiles()
                                            .then(async (uploadedFiles) => {
                                                if (uploadedFiles.length > 0) {
                                                    let id = uploadedFiles[0].fileName // here fileName is actually fileID as required and explained in the docs
                                                    let newFileName = generateNewImageName("interior-lighting-light-belt")
