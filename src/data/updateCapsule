                                                    record.chineseStandardsImage = uploadedFiles[0].fileUrl; // Save the uploaded image URL
                                                    $w('#updateBuildingCCChineseSC4').src = uploadedFiles[0].fileUrl
                                                }
                                            });
                                        uploadPromises.push(chineseStandardsImagePromise);
                                    }

                                    if (aericanStandardsImage.value.length > 0) {
                                        // Upload the AC image
                                        const aericanStandardsImagePromise = aericanStandardsImage.uploadFiles()
                                            .then(async (uploadedFiles) => {
                                                if (uploadedFiles.length > 0) {
                                                    let id = uploadedFiles[0].fileName // here fileName is actually fileID as required and explained in the docs
                                                    let newFileName = generateNewImageName("building-compliance-certificates-american")
