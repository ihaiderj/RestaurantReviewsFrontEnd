                                                    $w('#updateAppliancesFridgeIC4').src = uploadedFiles[0].fileUrl;
                                                }
                                            });
                                        uploadPromises.push(fridgeImagePromise);
                                    }
                                    if (cooktopImage.value.length > 0) {
                                        // Upload the AC image
                                        const cooktopImagePromise = cooktopImage.uploadFiles()
                                            .then(async (uploadedFiles) => {
                                                if (uploadedFiles.length > 0) {
                                                    let id = uploadedFiles[0].fileName // here fileName is actually fileID as required and explained in the docs
                                                    let newFileName = generateNewImageName("appliances-cooktop")
